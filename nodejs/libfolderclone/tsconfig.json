{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "es2016",
         "esnext.asynciterable",
         "dom"
      ],
      "target": "es2016",
      "module": "amd",
      "moduleResolution": "node",
      "outDir": "./build",
